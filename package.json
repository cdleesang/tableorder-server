    "@nestjs/swagger": "^7.3.1",
    "@oz-k/node-firebird-cp949": "1.1.10",
    "@prisma/client": "^5.12.0",
    "axios": "^1.6.8",
    "bcryptjs": "^2.4.3",
    "joi": "^17.12.3",
    "nanoid": "^3.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "typia": "^6.0.6"
